<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup Label="VTK Libraries">
    <Link>
      <AdditionalDependencies>
        $(MSBuildThisFileDirectory)$<TARGET_LINKER_FILE_NAME:@vtk-module@>;
        %(AdditionalDependencies)
      </AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <PropertyGroup>
    <@vtk-module@Found Condition="Exists('$(MSBuildThisFileDirectory)$<TARGET_LINKER_FILE_NAME:@vtk-module@>')">true</@vtk-module@Found>
  </PropertyGroup>
</Project>
