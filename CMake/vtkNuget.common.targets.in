<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)include\$(VisualStudioVersion)\$(Platform)
      </AdditionalIncludeDirectories>
    </ClCompile>
    <ResourceCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)include\$(VisualStudioVersion)\$(Platform)
      </AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <PropertyGroup>
    <HasSpecific@module@>false</HasSpecific@module@>
    <HasSpecific@module@ Condition="Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\Custom@module@.targets')">true</HasSpecific@module@>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\Custom@module@.targets"
          Condition="$(HasSpecific@module@)" />
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\Release\Custom@module@.targets"
          Condition="!$(HasSpecific@module@) And Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\Release\Custom@module@.targets')" />
</Project>
