<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)$(VisualStudioVersion)\$(Platform)\include
      </AdditionalIncludeDirectories>
    </ClCompile>
    <ResourceCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)$(VisualStudioVersion)\$(Platform)\include
      </AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <PropertyGroup>
    <@module@Found>false</@module@Found>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@module@.targets"
          Condition="Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@module@.targets')" />
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\Release\@module@.targets"
          Condition="!$(@module@Found) And Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@module@.targets')" />
  <Target Name="Ensure@module@Exists" BeforeTargets="PrepareForBuild">
    <Error Condition="$(@module@Found)"
           Text="This project references a VTK configuration ($(VisualStudioVersion) ($Platform) $(Configuration)) that is not provided by this NuGet package." />
  </Target>
</Project>
