<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)$(VisualStudioVersion)\$(Platform)\include
      </AdditionalIncludeDirectories>
    </ClCompile>
    <ResourceCompile>
      <AdditionalIncludeDirectories>
        %(AdditionalIncludeDirectories);
        $(MSBuildThisFileDirectory)include;
        $(MSBuildThisFileDirectory)$(VisualStudioVersion)\$(Platform)\include
      </AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <PropertyGroup>
    <@vtk-module@Found>false</@vtk-module@Found>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@vtk-module@.targets"
          Condition="Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@vtk-module@.targets')" />
  <Import Project="$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\Release\@vtk-module@.targets"
          Condition="!@vtk-module@Found And Exists('$(MSBuildThisFileDirectory)lib\$(VisualStudioVersion)\$(Platform)\$(Configuration)\@vtk-module@.targets')" />
  <Target Name="Ensure@vtk-module@Exists" BeforeTargets="PrepareForBuild">
    <Error Condition="@vtk-module@Found"
           Text="This project references a VTK configuration ($(VisualStudioVersion) ($Platform) $(Configuration)) that is not provided by this NuGet package." />
  </Target>
</Project>
